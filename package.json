{
  "name": "clang.js",
  "version": "0.1.1",
  "description": "Run C/C++ in the Browser.",
  "main": "dist/clang.cjs.js",
  "module": "dist/clang.js",
  "scripts": {
    "dev": "rollup -c -w --environment ENV:development --bundleConfigAsCjs",
    "build": "rollup -c --environment ENV:production --bundleConfigAsCjs",
    "clean": "rimraf dist/*"
  },
  "keywords": [
    "webassembly",
    "clang",
    "cpp"
  ],
  "files": [
    "dist"
  ],
  "author": "luoxuhai <darkce97@gmail.com> (https://github.com/luoxuhai)",
  "license": "Apache-2.0",
  "devDependencies": {
    "@rollup/plugin-replace": "^5.0.2",
    "@rollup/plugin-terser": "^0.4.3",
    "rimraf": "^5.0.1",
    "rollup": "^2.79.2",
    "rollup-plugin-copy": "^3.4.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-serve": "^2.0.2",
    "rollup-plugin-web-worker-loader": "^1.6.1"
  }
}
